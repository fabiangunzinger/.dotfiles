@article{petersen1994benefits,
	title        = {{ the Benefits of Lending Relationships: Evidence From Small Business Data}},
	author       = {Petersen, Mitchell A and Rajan, Raghuram G},
	year         = {1994},
	month        = {March},
	journal      = {Journal of Finance},
	volume       = {49},
	number       = {1},
	pages        = {3--37},
	url          = {https://ideas.repec.org/a/bla/jfinan/v49y1994i1p3-37.html},
	abstract     = {This paper empirically examines how ties between a firm and its creditors affect the availability and cost of funds to the firm. The authors analyze data collected in a survey of small firms by the Small Business Administration. The primary benefit of building close ties with an institutional creditor is that the availability of financing increases. The authors find smaller effects on the price of credit. Attempts to widen the circle of relationships by borrowing from multiple leaders increases the price and reduces the availability of credit. In sum, relationships are valuable and appear to operate more through quantities rather than prices. Copyright 1994 by American Finance Association.},
	date-added   = {2016-11-11 13:42:40 +0000},
	date-modified = {2016-11-11 13:42:54 +0000},
	bdsk-url-1   = {https://ideas.repec.org/a/bla/jfinan/v49y1994i1p3-37.html}
}

@article{petersen2002distance,
	title        = {{Does Distance Still Matter? the Information Revolution in Small Business Lending}},
	author       = {Mitchell A. Petersen and Raghuram G. Rajan},
	year         = {2002},
	month        = {December},
	journal      = {Journal of Finance},
	volume       = {57},
	number       = {6},
	pages        = {2533--2570},
	url          = {https://ideas.repec.org/a/bla/jfinan/v57y2002i6p2533-2570.html},
	abstract     = {The distance between small firms and their lenders is increasing, and they are communicating in more impersonal ways. After documenting these systematic changes, we demonstrate they do not arise from small firms locating differently, consolidation in the banking industry, or biases in the sample. Instead, improvements in lender productivity appear to explain our findings. We also find distant firms no longer have to be the highest quality credits, indicating they have greater access to credit. The evidence indicates there has been substantial development of the financial sector, even in areas such as small business lending. Copyright The American Finance Association 2002.},
	date-added   = {2016-11-11 13:02:18 +0000},
	date-modified = {2016-11-11 13:02:36 +0000},
	bdsk-url-1   = {https://ideas.repec.org/a/bla/jfinan/v57y2002i6p2533-2570.html}
}

