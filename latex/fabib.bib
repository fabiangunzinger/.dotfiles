@article{aymanns2015contagious,
	title        = {Contagious Synchronization and Endogenous Network Formation in Financial Networks},
	author       = {Christoph Aymanns and Co-Pierre Georg},
	year         = {2015},
	journal      = {Journal of Banking \& Finance},
	volume       = {50},
