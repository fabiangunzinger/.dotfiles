@incollection{bernanke1999financial,
	title        = {{The Financial Accelerator in a Quantitative Business Cycle Framework}},
	author       = {Bernanke, Ben S. and Gertler, Mark and Gilchrist, Simon},
	year         = {1999},
	booktitle    = {{Handbook of Macroeconomics}},
	publisher    = {Elsevier},
	series       = {Handbook of Macroeconomics},
	volume       = {1},
	pages        = {1341--1393},
	url          = {https://ideas.repec.org/h/eee/macchp/1-21.html},
	abstract     = {This chapter develops a dynamic general equilibrium model that is intended to help clarify the role of credit market frictions in business fluctuations, from both a qualitative and a quantitative standpoint. The model is a synthesis of the leading approaches in the literature. In particular, the framework exhibits a \&quot;financial accelerator\&quot;, in that endogenous developments in credit markets work to amplify and propagate shocks to the macroeconomy. In addition, we add several features to the model that are designed to enhance the empirical relevance. First, we incorporate money and price stickiness, which allows us to study how credit market frictions may influence the transmission of monetary policy. In addition, we allow for lags in investment which enables the model to generate both hump-shaped output dynamics and a lead-lag relation between asset prices and investment, as is consistent with the data. Finally, we allow for heterogeneity among firms to capture the fact that borrowers have differential access to capital markets. Under reasonable parametrizations of the model, the financial accelerator has a significant influence on business cycle dynamics.},
	chapter      = {21},
	date-added   = {2016-12-28 11:17:45 +0000},
	date-modified = {2016-12-28 11:17:51 +0000},
	editor       = {J. B. Taylor and M. Woodford},
	bdsk-url-1   = {https://ideas.repec.org/h/eee/macchp/1-21.html}
}

