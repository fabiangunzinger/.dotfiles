@article{anand2013network,
	title        = {A Network Model of Financial System Resilience},
	author       = {Kartik Anand and Prasanna Gai and Sujit Kapadia and Simon Brennan and Matthew Willison},
	year         = {2013},
	journal      = {Journal of Economic Behavior \& Organization},
	volume       = {85},
